    "space-flight-now-scraper": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.2",
    "babel-jest": "14.1.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-3": "6.11.0",
    "eslint": "3.4.0",
    "eslint-config-dashbot-backend": "1.0.1",
    "eslint-plugin-async": "0.1.1",
    "eslint-plugin-node": "2.0.0",
    "grunt": "^1.0.1",
    "grunt-aws-lambda": "^0.13.0",
    "grunt-cli": "^1.2.0",
    "node-lambda": "0.8.5",
